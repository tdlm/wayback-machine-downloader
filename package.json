{
  "name": "wayback-dl",
  "version": "1.0.0",
  "description": "Download archived websites from the Internet Archive Wayback Machine",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "wayback-dl": "bin/wayback-dl.js"
  },
  "files": [
    "dist",
    "bin"
  ],
  "scripts": {
    "build": "tsc",
    "prepare": "pnpm run build",
    "test": "node --import tsx --test test/*.test.ts",
    "start": "node --import tsx bin/wayback-dl.js",
    "dev": "node --import tsx src/cli.ts"
  },
  "keywords": [
    "wayback-machine",
    "internet-archive",
    "archive",
    "download",
    "cli"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/example/wayback-dl.git"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "cli-progress": "^3.12.0",
    "commander": "^12.1.0",
    "p-queue": "^8.0.1"
  },
  "devDependencies": {
    "@types/cli-progress": "^3.11.6",
    "@types/node": "^22.10.1",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2"
  }
}
